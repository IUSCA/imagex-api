{
  "name": "process",
  "plural": "processes",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "default": "new_process"
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "NEW"
    },
    "progress": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "owner": {
      "type": "string",
      "default": "Nobody"
    },
    "refID": {
      "type": "string",
      "default": "None"
    },
    "parentID": {
      "type": "string",
      "default": "None"
    },
    "started_at": {
      "type": "date",
      "required": true
    },
    "ended_at": {
      "type": "date"
    },
    "last_updated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
